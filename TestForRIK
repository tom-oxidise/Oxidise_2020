        for( $posx = 13 ;  $posx < 170 ; $posx = $posx + 95 ) { 
        $interSpaceHeight = 5;  
          if ( $boxCount < 4 ) {
            if ( $boxCount % 2 == 1 ) {
              $boxCount++;
              continue;
            }
            $doubleBox = true;
            $boxWidth = 90 + 90 + 5;
            if ( $boxCount == 0 ) {
              $boxHeight = 60;
              $interSpaceHeight = 5;
            } else if ( $boxCount == 2 ) {
              $boxHeight = 50;
            }
            
          } else {
            $doubleBox = false;
            $boxWidth = 90;
